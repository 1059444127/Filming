<Window x:Class="UIH.Mcsf.Filming.PreFilmingProgressDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Filming="clr-namespace:UIH.Mcsf.Filming"
             mc:Ignorable="d" 
             d:DesignHeight="120" d:DesignWidth="720" Height ="100" Width ="600"
        ShowInTaskbar="False" WindowStartupLocation="CenterScreen" WindowStyle="None" AutomationProperties.AutomationId="ID_EX_FILMING_PREFILMING_PROGRESSDIALOG">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Label Content="preprocessing before printing films..." AutomationProperties.AutomationId="ID_LBL_FILMING_PREPROCESSING_BEFORE_PRINTINGFILMS" HorizontalAlignment="Center"  Name="label" VerticalAlignment="Center" />
        </Grid>
        <Grid Grid.Row="1">
            <!--ProgressBar   HorizontalAlignment="Stretch"  Name="progressBar" VerticalAlignment="Stretch" SmallChange="1" LargeChange="10" /-->
            <Filming:PreFilmingProgressControl />
        </Grid>
    </Grid>
</Window>
